package assets.heaven.dimension;

import java.util.Random;

import net.minecraft.world.World;
import net.minecraft.world.gen.structure.StructureStart;

public class StructureMineshaftStartHeaven extends StructureStart 
{
	public StructureMineshaftStartHeaven(World worldObj, Random rand, int par1, int par2) {}
	
    @SuppressWarnings("unchecked")
	public StructureMineshaftStartHeaven(World par1World, Random par2Random, int par3, int par4)
    {
    	super(par3, par4);
    	StructureMineshaftPiecesHeaven.Room var5 = new StructureMineshaftPiecesHeaven.Room(0, par2Random, (par3 << 4) + 2, (par4 << 4) + 2);
        this.components.add(var5);
        var5.buildComponent(var5, this.components, par2Random);
        this.updateBoundingBox();
        this.markAvailableHeight(par1World, par2Random, 10);
    }
}
