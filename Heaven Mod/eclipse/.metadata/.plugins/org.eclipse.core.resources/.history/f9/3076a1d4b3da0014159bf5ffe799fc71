	public static void mainRegistry()
	{
		addCraftingRecipe();
		addSmeltingRecipe();
	}
	
	public static void addCraftingRecipe()
	{
		//Shaped
		GameRegistry.addRecipe(new ItemStack(), params);;
		
	}
	
	public static void addSmeltingRecipe()
	{
		
	}
}
