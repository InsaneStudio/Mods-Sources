package com.heaven.blocks;

import cpw.mods.fml.common.registry.GameRegistry;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class SkyStone {

	public static void mainRegistry()
	{
		initializeItem();
		registerItem();
	}
	public static Block skyStone;
	
	public static void initializeItem()
	{
		skystone = new Block().setUnlocalizedName("").setCreativeTab(CreativeTabs.tabMisc);
	}
	public static void registerItem()
	{
		GameRegistry.registerItem(heavenLightier, heavenLightier.getUnlocalizedName());
	}
}